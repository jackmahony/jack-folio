{% extends "base.twig" %}{% block content %}{% include 'particles/navigation.twig' %}<section class="secondary" id="PortfolioHeader"><div class="row"><div class="medium-centered medium-8 columns">{% if post.portfolio_header %}<h1>{{post.portfolio_header}}</h1>{% endif %}</div></div></section><section id="ProjectThumbnails"><div class="row medium-up-2">{% for project in post.get_field('project_thumbnail') %}<div class="column"><div class="projectthumbnail"><img src="{{TimberImage(project.project_image)|resize(280, 233) }}" /><h3>{{project.project_title}}</h3><p>{{project.company_name}}</h6></p></div></div>{% endfor %}</div></section>{% endblock %}