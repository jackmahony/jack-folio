{% extends "base.twig" %}{% block content %}<div class="portfolio-nav">{% include 'particles/navigation.twig' %}</div><section id="PortfolioHeader"><div class="row align-center"><div class="medium-8 columns">{% if post.portfolio_header %}<h1>{{post.portfolio_header}}</h1><h2 class="mark">UX Projects</h2>{% endif %}</div></div></section><section id="ProjectThumbnails"><div class="row small-up-1 medium-up-3">{% for project in projects |slice (0,3) %}<div class="column"><div class="projectthumbnail wow fadeInUp"><a href="{{project.link}}" class="project"><div class="img">{% if project.project_image %}<img src="{{TimberImage(project.project_image)|resize(280, 210) }}" />{% endif %}</div><div class="info">{% if project.project_title %}<h3>{{project.project_title}}</h3>{% endif %}{% if project.company_name %}<p>{{project.company_name}}</p>{% endif %}</div></a></div></div>{% endfor %}</div><div class="row align-center"><h2 class="mark second">UI Design/Web Dev</h2></div><div class="row small-up-1 medium-up-2">{% for project in projects |slice (3) %}<div class="column"><div class="projectthumbnail2 wow fadeInUp"><a href="{{project.link}}" class="project"><div class="img">{% if project.project_image %}<img src="{{TimberImage(project.project_image)|resize(480, 210) }}" />{% endif %}</div><div class="info">{% if project.project_title %}<h3>{{project.project_title}}</h3>{% endif %}{% if project.company_name %}<p>{{project.company_name}}</p>{% endif %}</div></a></div></div>{% endfor %}</div></section>{% include 'particles/footer.twig' %}{% endblock %}