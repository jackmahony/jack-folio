{% extends "base.twig" %}{% block content %}{% include 'particles/navigation.twig' %}<section id="PortfolioHeader"><div class="row align-center"><div class="medium-8 columns">{% if post.portfolio_header %}<h1 class="mark">{{post.portfolio_header}}</h1>{% endif %}</div></div></section><section id="ProjectThumbnails"><div class="row small-up-1 medium-up-3">{% for project in projects %}<div class="column"><div class="projectthumbnail"><a href="{{project.link}}" class="project">{% if project.project_image %}<img src="{{TimberImage(project.project_image)|resize(280, 210) }}" />{% endif %}{% if project.project_title %}<h3>{{project.project_title}}</h3>{% endif %}{% if project.company_name %}<p>{{project.company_name}}</p>{% endif %}<div class="foot"></div></a></div></div>{% endfor %}</div></section>{% include 'particles/footer.twig' %}{% endblock %}