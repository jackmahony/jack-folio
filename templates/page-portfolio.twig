{% extends "base.twig" %}{% block content %}{% include 'particles/navigation.twig' %}<section class="secondary" id="PortfolioHeader"><div class="row align-center"><div class="medium-8 columns">{% if post.portfolio_header %}<h1>{{post.portfolio_header}}</h1>{% endif %}</div></div></section><section id="ProjectThumbnails"><div class="row small-up-1 medium-up-2">{% for project in projects %}<div class="column"><a href="{{project.link}}" class="project"><div class="projectthumbnail">{% if project.project_image %}<img src="{{TimberImage(project.project_image)|resize(280, 233) }}" />{% endif %}{% if project.project_title %}<h3>{{project.project_title}}</h3>{% endif %}{% if project.company_name %}<p>{{project.company_name}}</p>{% endif %}</div></a></div>    {% endfor %}</div></section>{% endblock %}