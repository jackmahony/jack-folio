{% extends "base.twig" %}{% block content %}{% include 'particles/navigation.twig' %}<section style="background: url('{{ TimberImage(post.header_image)}}')center;" id="ProjectIntroduction"><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.title %}<h2>{{post.title}}</h2>{% endif %}{% if post.about %}<p>{{post.about}}</p>{% endif %}</div></div></section><section id="ResearchPlan"><div class="row align-center"><div class="medium-12 columns">{% if post.research %}<h2>{{post.research}}</h2>{% endif %}{% for item in post.get_field('research_areas') %}<div class="research"><h3>{{item.title}}</h3><p>{{item.about}}</p></div>{% endfor %}</div></div></section><section class="secondary" id="GenerativeData"><div class="row align-center"><div class="small-12 medium-6 columns">{% if post.gdata %}<p>{{post.gdata}}</p>{% endif %}</div></div><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.gdata2 %}<p>{{post.gdata2}}</p>{% endif %}</div></div></section><section id="ResearchApproach"><br /><br /></section><section class="secondary" id="CompetitorAnalysis"><div class="row"><div class="small-12 medium-4 columns">{% if post.competitor_analysis_title %}<h2>{{post.competitor_analysis_title}}</h2>{% endif %}{% if post.competitor_analysis_text %}<p>{{post.competitor_analysis_text}}</p>{% endif %}</div><div class="small-12 medium-8 columns">{% for image in post.get_field('competitor_logos') %}<img src="{{ TimberImage(image)|resize(200, 200) }}" />{% endfor %}</div></div><div class="row"><div class="medium-12 columns">{% if post.screen_captures_title %}<h2>{{post.screen_captures_title}}</h2>{% endif %}{% for image in post.get_field('screen_captures_images') %}<img src="{{ TimberImage(image) }}" />{% endfor %}</div></div></section><section id="AffinityMapping"><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.affinity_mapping_title %}<h2>{{post.affinity_mapping_title}}</h2>{% endif %}{% if post.affinity_mapping_text %}<p>{{post.affinity_mapping_text}}</p>{% endif %}</div></div><div class="row"><div class="medium-12 columns">{% if post.affinity_map_image %}<img src="{{ TimberImage(post.affinity_map_image) }}" />{% endif %}{% for image in post.get_field('affinity_map_stickys') %}<img src="{{ TimberImage(image) }}" />{% endfor %}</div></div></section><section class="secondary" id="DevelopingUserPersonas"><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.user_personas_title %}<h2>{{post.user_personas_title}}</h2>{% endif %}{% if post.user_personas_text %}<p>{{post.user_personas_text}}</p>{% endif %}</div></div><div class="row"><div class="medium-12 columns">{% if post.user_persona_image %}<img src="{{ TimberImage(post.user_persona_image) }}" />{% endif %}</div></div><div class="row"><div class="small-12 medium-6 columns">{% if post.defining_the_problem %}<p>{{post.defining_the_problem}}</p>{% endif %}</div><div class="small-12 medium-6 columns">{% if post.how_can_we_improve_this %}<p>{{post.how_can_we_improve_this}}</p>{% endif %}</div></div></section><section id="StoryBoards"><div class="row align-right"><div class="small-12 medium-8 columns">{% if post.story_board_image %}<img src="{{ TimberImage(post.story_board_image) }}" />{% endif %}</div></div><div class="row align-left"><div class="small-12 medium-8 columns">{% if post.story_board_image2 %}<img src="{{ TimberImage(post.story_board_image2) }}" />{% endif %}</div></div><div class="row align-center"><div class="medium-8 columns">{% if post.feature_prioritisation %}<p>{{post.feature_prioritisation}}</p>{% endif %}</div></div><div class="row align-center"><div class="small-12 medium-10 columns">{% if post.feature_prioritisation_image %}<img src="{{ TimberImage(post.feature_prioritisation_image) }}" />{% endif %}</div></div><div class="row align-center"><div class="medium-12 columns">{% if post.user_flow_title %}<h2>{{post.user_flow_title}}</h2>{% endif %}{% if post.user_flow_image %}<img src="{{ TimberImage(post.user_flow_image) }}" />{% endif %}</div></div></section><section class="secondary" id="PaperPrototype"><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.test_title %}<h2>{{post.test_title}}</h2>{% endif %}{% if post.test_text %}<p>{{post.test_text}}</p>{% endif %}</div></div><div class="row small-up-1 medium-up-2">{% for image in post.get_field('paper_prototypes') %}<div class="column"><img src="{{ TimberImage(image) }}" /></div>{% endfor %}</div></section><section id="FeaturePrioritisation"><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.feature_title %}<h2>{{post.feature_title}}</h2>{% endif %}{% if post.feature_text %}<p>{{post.feature_text}}</p>{% endif %}</div></div></section><section class="secondary" id="CalendarFeature"><div class="row"><div class="medium-12 columns">{% if post.calendar_feature %}<h2>{{calendar_feature}}</h2>{% endif %}{% if post.calendar_image %}<img src="{{ TimberImage(post.calendar_image) }}" />{% endif %}</div></div></section><section id="PivotingDirection"><div class="row"><div class="small-12 medium-6 columns">{% if post.pivot_image %}<img src="{{ TimberImage(post.pivot_image ) }}" />{% endif %}</div><div class="small-12 medium-6 columns">{% if post.pivot_text %}<p>{{post.pivot_text}}</p>{% endif %}</div></div><div class="row"><div class="small-12 medium-6 columns">{% if post.altering_problem_text %}<p>{{post.altering_problem_text}}</p>{% endif %}</div><div class="small-12 medium-6 columns">{% if post.altering_problem_image %}<img src="{{ TimberImage(post.altering_problem_image ) }}" />{% endif %}</div></div></section><section class="secondary" id="PersonaScenarios"><div class="row Chelsea"><div class="small-12 medium-6 columns">{% if post.chelsea_persona_title %}<h2>{{post.chelsea_persona_title}}</h2>{% endif %}{% if post.chelsea_persona_text %}<p>{{post.chelsea_persona_text}}</p>{% endif %}</div><div class="small-12 medium-6 columns">{% for image in post.get_field('chelsea_persona_images') %}<img src="{{ TimberImage(image) }}" />{% endfor %}</div></div><div class="row Steve"><div class="small-12 medium-6 columns">{% for image in post.get_field('steve_persona_images') %}<img src="{{ TimberImage(image) }}" />{% endfor %}</div><div class="small-12 medium-6 columns">{% if post.steve_persona_title %}<h2>{{post.steve_persona_title}}</h2>{% endif %}{% if post.steve_persona_text %}<p>{{post.steve_persona_text}}</p>{% endif %}</div></div><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.quote %}<h3>{{post.quote}}</h3>{% endif %}</div></div></section><section id="OpenCardSorting"><div class="row align-center"><div class="small-12 medium-8">{% if post.open_card_sorting %}<p>{{post.open_card_sorting}}</p>{% endif %}</div></div><div class="row"><div class="medium-12 columns">{% for image in post.get_field('open_card_images') %}<img src="{{ TimberImage(image) }}" />{% endfor %}</div></div><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.proposed_sitemap %}<h2>{{post.proposed_sitemap }}</h2>{% endif %}</div></div><div class="sitemap">{% if post.sitemap_image %}<img src="{{ TimberImage(post.sitemap_image) }}" />{% endif %}</div></section><section class="secondary" id="ChosenTechnology"><div class="row"><div class="small-12 medium-6 columns">{% if post.what_tech %}<p>{{post.what_tech }}</p>{% endif %}</div><div class="small-12 medium-6 columns">  IMAGE TO COME!</div></div></section><section id="DevelopingWireframes"><div class="row"><div class="small-12 medium-6 columns">  IMAGE TO COME!</div><div class="small-12 medium-6 columns">{% if post.developing_wireframes %}<p>{{post.what_tech}}</p>{% endif %}</div></div></section><section class="secondary" id="WireframeScreens"><div class="row"><div class="medium-12 columns">{% for item in post.get_field('key_screens') %}<h2>{{item.title}}</h2><img src="{{TimberImage(item.screen).src}}" />{% endfor %}</div></div><div class="row align-center"><div class="medium-8 columns">{% if post.testing %}<p>{{post.testing}}</p>{% endif %}</div></div><div class="row small-up-1 medium-up-2">{% for image in post.get_field('prototype_images') %}<div class="column"><img src="{{ TimberImage(image) }}" /></div>{% endfor %}</div><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.key_design_iterations %}<p>{{post.key_design_iterations}}</p>{% endif %}</div></div></section><section id="FinalProduct"><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.usability_and_testing %}<p>{{post.usability_and_testing}}</p>{% endif %}</div></div><div class="row align-center"><div class="small-12 medium-8 columns">{% if post.the_final_product %}<p>{{post.the_final_product}}</p>{% endif %}</div></div></section>{% endblock %}